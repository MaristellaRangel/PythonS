from random import randint

jogos = {}

for i in range(4):
    dado = randint(1, 6)
    jogos["Jogador" + str(i + 1)] = dado

for chave, valor in jogos.items():
    print(f"O {chave} sorteou {valor}")
print("-" * 50)

cont = 1
for i in sorted(jogos, key=jogos.get, reverse=True):
    print(f"O {cont}Â° lugar: {i} com {jogos[i]}")
    cont += 1
