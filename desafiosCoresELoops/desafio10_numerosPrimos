num = int(input("Digite um número:"))
divisores = 0
for i in range(1, num+1):
    if num % i == 0:
        print(f"\033[1;32m {i}\033[m", end="")
        divisores += 1
    else:
        print(f"\033[1;31m {i}\033[m", end="")
if divisores >= 2:
    print(f"\nO número {num} é primo")
else:
    print(f"\nO número {num} não é primo")
