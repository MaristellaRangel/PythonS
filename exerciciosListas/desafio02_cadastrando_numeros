numeros = []
while(True):
    numero = int(input("Digite um n√∫mero: "))
    if numero  not in numeros:
        numeros.append(numero)
        print(f"Sucesso... Lita = {numeros}")
    else:
        print(f"Duplicado... Lista = {numeros}")

    resposta = input("Deseja continuar [S/N]: ").lower()
    if resposta[0] != 's':
        numeros.sort()
        print(f"Lista = {numeros}")
        break
