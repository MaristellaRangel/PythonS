cores = {
    'Reprovado' : '\33[1;31;43m',
    'Aprovado' :  '\33[1;34;43m',
    'Limpa' :      '\33[m'
}

media = float(input("Digite a média: "))
freq = float(input("Digite a frequência: "))

if media >= 50 and freq >= 75:
   r = "Aprovado"
else:
    r = "Reprovado"

print(f"Média {cores[r]} {media} {cores['Limpa']}")
